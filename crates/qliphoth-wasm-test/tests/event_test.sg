// Event System Test

pub fn main() -> i64 {
    console::log_str("=== Event System Test ===");

    // Create a button element
    let button = dom::create_element("button");
    console::log_str("Created button element");

    // Add click event listener with callback
    let listener_id = events::add_listener(button, "click", || {
        console::log_str("Button clicked!");
    }, 0);

    console::log_str("Registered click listener");

    // For testing purposes, we can't actually dispatch events without DOM
    // but the compilation should work

    0
}
