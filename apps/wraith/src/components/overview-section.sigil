                ·child(
                VNode·div()
                    ·class("p-2 space-y-1")
                    // Map: ∀ alert ∈ mockAlerts.slice(0, 4)
                    ·children(mock_alerts.slice(0, 4).iter().map(|alert| ActiveAlert·view()
                            ·attr("key", alert.id)
                            ·attr("alert", alert)).collect())
                )
            )
        )
