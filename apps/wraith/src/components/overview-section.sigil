                ·child(
                VNode·div()
                    ·class("p-2")
                    // Map: ∀ pipeline ∈ mockPipelines.slice(0, 4)
                    ·children(mock_pipelines.slice(0, 4).iter().map(|pipeline| RecentPipelineRun·view()
                            ·attr("key", pipeline.id)
                            ·attr("pipeline", pipeline)).collect())
                )
            )
            ·child(
            VNode·div()
                ·class("bg-[var(--bg-secondary)] rounded-lg border border-[var(--border-primary)] overflow-hidden")
                ·child(
                VNode·div()
                    ·class("flex items-center justify-between px-4 py-3 border-b border-[var(--border-subtle)]")
                    ·child(
                    VNode·div()
                        ·class("flex items-center gap-2")
                        ·child(
                        VNode·h3()
                            ·class("text-sm font-medium text-[var(--text-primary)]")
                            ·text_child("Active Alerts")
                        )
                        ·when(firing_alerts > 0, VNode·span()
                            ·class("px-1.5 py-0.5 text-xs bg-[var(--accent-error)]/20 text-[var(--accent-error)] rounded")
                            ·text_child(firing_alerts·to_string())
                            ·text_child("firing"))
                    )
                    ·child(
                    VNode·button()
                        ·on_click(Click)
                        ·class("text-xs text-[var(--accent-primary)] hover:underline")
                        ·text_child("View all")
