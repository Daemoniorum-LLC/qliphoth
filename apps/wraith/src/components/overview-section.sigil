invoke qliphoth·prelude·*;
rite overview_section(on_navigate: Any) -> VNode! {
    VNode·div()
        ·class("h-full overflow-auto pr-1")
        ·child(
        VNode·div()
            ·class("mb-6")
            ·child(
            VNode·h2()
                ·class("text-lg font-semibold text-[var(--text-primary)] mb-1")
                ·text_child("DevOps Overview")
            )
            ·child(
            VNode·p()
                ·class("text-sm text-[var(--text-secondary)]")
