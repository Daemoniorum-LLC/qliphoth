            )
        )
        ·child(
        VNode·div()
            ·class("grid grid-cols-2 lg:grid-cols-4 gap-3 mb-6")
            ·child(
            StatCard·view()
                ·attr("title", "Containers")
                ·attr("value", mock_containers.len())
                ·attr("icon", None)
                ·attr("items", [
            ∅,
            ∅,
          ])
                ·on_click(Click)
            )
            ·child(
            StatCard·view()
                ·attr("title", "Pipelines")
                ·attr("value", mock_pipelines.len())
                ·attr("icon", None)
                ·attr("items", [
            ∅,
            ∅,
            ∅,
          ])
                ·on_click(Click)
            )
            ·child(
            StatCard·view()
                ·attr("title", "Cloud Providers")
                ·attr("value", mock_providers.len())
                ·attr("icon", None)
                ·attr("items", [
            ∅,
            ∅,
          ])
                ·on_click(Click)
            )
            ·child(
            StatCard·view()
                ·attr("title", "Infra Stacks")
                ·attr("value", mock_infra_stacks.len())
                ·attr("icon", None)
                ·attr("items", [
            ∅,
          ])
                ·on_click(Click)
            )
        )
        ·child(
        VNode·div()
            ·class("grid grid-cols-1 lg:grid-cols-2 gap-6")
            ·child(
            VNode·div()
                ·class("bg-[var(--bg-secondary)] rounded-lg border border-[var(--border-primary)] overflow-hidden")
