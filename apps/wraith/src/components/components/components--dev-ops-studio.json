    "code": "import { useState } from 'react'\nimport {\n  LayoutDashboard, Box, Layers, GitBranch, Cloud, Boxes, Key, Activity\n} from 'lucide-react'\nimport type { DevOpsSection } from '../types'\nimport { OverviewSection } from './overview'\nimport { ContainersSection, ImagesSection, ComposeSection } from './containers'\nimport { PipelinesSection } from './pipelines'\nimport { ProvidersSection } from './providers'\nimport { InfrastructureSection, SecretsSection } from './infrastructure'\nimport { MonitoringSection } from './monitoring'\n\ninterface NavItem {\n  id: DevOpsSection\n  label: string\n  icon: React.ReactNode\n}\n\nconst navItems: NavItem[] = [\n  { id: 'overview', label: 'Overview', icon: <LayoutDashboard className=\"h-4 w-4\" /> },\n  { id: 'containers', label: 'Containers', icon: <Box className=\"h-4 w-4\" /> },\n  { id: 'images', label: 'Images', icon: <Layers className=\"h-4 w-4\" /> },\n  { id: 'compose', label: 'Compose', icon: <Layers className=\"h-4 w-4\" /> },\n  { id: 'pipelines', label: 'Pipelines', icon: <GitBranch className=\"h-4 w-4\" /> },\n  { id: 'providers', label: 'Providers', icon: <Cloud className=\"h-4 w-4\" /> },\n  { id: 'infrastructure', label: 'Infrastructure', icon: <Boxes className=\"h-4 w-4\" /> },\n  { id: 'secrets', label: 'Secrets', icon: <Key className=\"h-4 w-4\" /> },\n  { id: 'monitoring', label: 'Monitoring', icon: <Activity className=\"h-4 w-4\" /> },\n]\n\nexport function DevOpsStudio() {\n  const [activeSection, setActiveSection] = useState<DevOpsSection>('overview')\n\n  const renderSection = () => {\n    switch (activeSection) {\n      case 'overview':\n        return <OverviewSection onNavigate={setActiveSection} />\n      case 'containers':\n        return <ContainersSection />\n      case 'images':\n        return <ImagesSection />\n      case 'compose':\n        return <ComposeSection />\n      case 'pipelines':\n        return <PipelinesSection />\n      case 'providers':\n        return <ProvidersSection />\n      case 'infrastructure':\n        return <InfrastructureSection />\n      case 'secrets':\n        return <SecretsSection />\n      case 'monitoring':\n        return <MonitoringSection />\n      default:\n        return <OverviewSection onNavigate={setActiveSection} />\n    }\n  }\n\n  return (\n    <div className=\"h-full flex flex-col bg-[var(--bg-primary)] text-[var(--text-primary)]\">\n      {/* Navigation Tabs */}\n      <div className=\"flex-shrink-0 border-b border-[var(--border-primary)] bg-[var(--bg-secondary)]\">\n        <div className=\"flex items-center overflow-x-auto scrollbar-thin\">\n          {navItems.map((item) => (\n            <button\n              key={item.id}\n              onClick={() => setActiveSection(item.id)}\n              className={`\n                flex items-center gap-2 px-4 py-2.5 text-xs font-medium whitespace-nowrap\n                border-b-2 transition-colors\n                ${activeSection === item.id\n                  ? 'border-[var(--accent-primary)] text-[var(--accent-primary)] bg-[var(--bg-primary)]'\n                  : 'border-transparent text-[var(--text-secondary)] hover:text-[var(--text-primary)] hover:bg-[var(--bg-hover)]'\n                }\n              `}\n            >\n              {item.icon}\n              {item.label}\n            </button>\n          ))}\n        </div>\n      </div>\n\n      {/* Content Area */}\n      <div className=\"flex-1 overflow-hidden p-4\">\n        {renderSection()}\n      </div>\n    </div>\n  )\n}\n",
