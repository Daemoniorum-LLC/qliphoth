    "code": "import { Card, Label } from \"@persona-framework/ui\"\nimport { History } from \"lucide-react\"\nimport type { PromptVersion } from \"../../types\"\n\ninterface PromptVersionHistoryProps {\n  versions: PromptVersion[]\n  onRestore: (version: PromptVersion) => void\n}\n\nexport function PromptVersionHistory({ versions, onRestore }: PromptVersionHistoryProps) {\n  return (\n    <div className=\"flex flex-col gap-3\">\n      <div className=\"flex items-center justify-between gap-2\">\n        <Label className=\"font-semibold\">Version History</Label>\n        <History className=\"h-5 w-5\" />\n      </div>\n\n      <div className=\"flex flex-col gap-2 max-h-[400px] overflow-auto\">\n        {versions.length === 0 && (\n          <div className=\"text-muted-foreground p-4 text-center\">\n            No version history yet. Versions are saved automatically when you make changes.\n          </div>\n        )}\n        {versions.map((version, index) => (\n          <Card\n            className=\"p-3 cursor-pointer transition-colors hover:bg-accent\"\n            key={version.id}\n            onClick={() => onRestore(version)}\n          >\n            <div className=\"flex items-center justify-between mb-1\">\n              <Label className=\"font-semibold\">\n                Version\n                {\" \"}\n                {versions.length - index}\n              </Label>\n              <div className=\"text-xs text-muted-foreground\">\n                {new Date(version.timestamp).toLocaleString()}\n              </div>\n            </div>\n            <div className=\"text-sm text-muted-foreground\">\n              {version.content.substring(0, 100)}\n              {version.content.length > 100 ? \"...\" : \"\"}\n            </div>\n            <div className=\"text-xs text-muted-foreground mt-1\">\n              Temp:\n              {\" \"}\n              {version.config.temperature}\n              {\" \"}\n              | Max tokens:\n              {\" \"}\n              {version.config.maxTokens}\n              {\" \"}\n              | Variables:\n              {\" \"}\n              {version.variables.length}\n            </div>\n          </Card>\n        ))}\n      </div>\n    </div>\n  )\n}\n",
