    "code": "import React, { useState, useEffect, useRef } from 'react'\nimport { Input, Dialog, DialogContent, DialogHeader, DialogTitle, DialogFooter, Button } from '@persona-framework/ui'\n\ninterface GoToLineDialogProps {\n  open: boolean\n  onClose: () => void\n  onGoToLine: (lineNumber: number) => void\n  maxLine?: number\n}\n\nexport function GoToLineDialog({ open, onClose, onGoToLine, maxLine }: GoToLineDialogProps) {\n  const [lineNumber, setLineNumber] = useState('')\n  const [error, setError] = useState('')\n  const inputRef = useRef<HTMLInputElement>(null)\n\n  useEffect(() => {\n    if (open && inputRef.current) {\n      setTimeout(() => {\n        inputRef.current?.focus()\n        inputRef.current?.select()\n      }, 100)\n      setLineNumber('')\n      setError('')\n    }\n  }, [open])\n\n  const handleSubmit = () => {\n    const num = parseInt(lineNumber)\n\n    if (isNaN(num) || num < 1) {\n      setError('Please enter a valid line number (1 or greater)')\n      return\n    }\n\n    if (maxLine && num > maxLine) {\n      setError(`Line number must be between 1 and ${maxLine}`)\n      return\n    }\n\n    onGoToLine(num)\n    onClose()\n  }\n\n  const handleKeyDown = (e: React.KeyboardEvent) => {\n    if (e.key === 'Enter') {\n      e.preventDefault()\n      handleSubmit()\n    } else if (e.key === 'Escape') {\n      e.preventDefault()\n      onClose()\n    }\n  }\n\n  return (\n    <Dialog open={open} onOpenChange={onClose}>\n      <DialogContent className=\"max-w-[400px] w-[90vw] p-5 bg-[#252526] border border-[#3c3c3c]\">\n        <DialogHeader>\n          <DialogTitle className=\"text-sm font-semibold text-[#cccccc]\">Go to Line</DialogTitle>\n        </DialogHeader>\n\n        <div className=\"space-y-4\">\n          <Input\n            ref={inputRef}\n            value={lineNumber}\n            onChange={(e) => {\n              setLineNumber(e.target.value)\n              setError('')\n            }}\n            onKeyDown={handleKeyDown}\n            placeholder={maxLine ? `Enter line number (1-${maxLine})` : 'Enter line number'}\n            type=\"number\"\n            className=\"w-full bg-[#3c3c3c] border-none text-[#cccccc] placeholder:text-[#858585]\"\n          />\n\n          {error && <div className=\"text-[#f48771] text-xs -mt-3\">{error}</div>}\n        </div>\n\n        <DialogFooter className=\"flex justify-end gap-2\">\n          <Button variant=\"outline\" onClick={onClose}>\n            Cancel\n          </Button>\n          <Button onClick={handleSubmit}>\n            Go\n          </Button>\n        </DialogFooter>\n      </DialogContent>\n    </Dialog>\n  )\n}\n",
