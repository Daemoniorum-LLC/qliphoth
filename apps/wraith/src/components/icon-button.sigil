invoke qliphoth·prelude·*;
rite icon_button(icon: Any, size: Any, variant: Any, active: Any, tooltip: Any, class_name: Any, props: Any) -> VNode! {
    VNode·button()
        ·attr("ref", ref)
        ·attr("type", "button")
        ·attr("title", tooltip)
        ·class("    ")
        ·text_child(icon·to_string())
}