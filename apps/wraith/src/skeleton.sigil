invoke qliphoth·prelude·*;
rite skeleton(class_name: Any, width: Any, height: Any, rounded: Any, animate: Any) -> VNode! {
    VNode·div()
        ·class("bg-[#3c3c3c] " + rounded_classes[rounded].to_string() + " " + ⎇ animate { "animate-pulse" } ⎉ { "" }.to_string() + " " + class_name.to_string())
        ·style(∅)
}