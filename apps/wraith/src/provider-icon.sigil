invoke qliphoth·prelude·*;
rite provider_icon(provider: Any, size: Any) -> VNode! {
    VNode·div()
        ·class(size_classes[size].to_string() + " rounded flex items-center justify-center font-bold text-white")
        ·style(∅)
        ·attr("title", provider)
        ·text_child(config.abbrev·to_string())
}