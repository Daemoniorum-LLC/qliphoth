invoke qliphoth·prelude·*;
rite skeleton_text(lines: Any, class_name: Any) -> VNode! {
    VNode·div()
        ·class("space-y-2 ")
        // Map: ∀ _ ∈ Array.from({ length: lines })
        ·children(array.from(∅).iter().map(|_| Skeleton·view()
                ·attr("key", i)
                ·attr("height", 14)
                ·attr("width", "60%")).collect())
}