    "code": "import { Input, Label, Slider, Textarea } from \"@persona-framework/ui\"\nimport type { PromptConfig } from \"../../types\"\n\ninterface ModelConfigPanelProps {\n  config: PromptConfig\n  onChange: (config: PromptConfig) => void\n}\n\nexport function ModelConfigPanel({ config, onChange }: ModelConfigPanelProps) {\n  return (\n    <div className=\"flex flex-col gap-4\">\n      <div className=\"flex flex-col gap-2\">\n        <div className=\"flex flex-col gap-1\">\n          <Label className=\"font-semibold\">\n            Temperature\n            {\" \"}\n            <span className=\"text-muted-foreground font-semibold\">{config.temperature.toFixed(2)}</span>\n          </Label>\n          <Slider\n            max={2}\n            min={0}\n            onValueChange={([value]) => onChange({ ...config, temperature: value })}\n            step={0.1}\n            value={[config.temperature]}\n          />\n        </div>\n      </div>\n\n      <div className=\"flex flex-col gap-2\">\n        <Label className=\"font-semibold\">Max Tokens</Label>\n        <Input\n          onChange={(e) => onChange({ ...config, maxTokens: Number.parseInt(e.target.value) || 0 })}\n          type=\"number\"\n          value={config.maxTokens.toString()}\n        />\n      </div>\n\n      <div className=\"flex flex-col gap-2\">\n        <div className=\"flex flex-col gap-1\">\n          <Label className=\"font-semibold\">\n            Top P\n            {\" \"}\n            <span className=\"text-muted-foreground font-semibold\">{config.topP.toFixed(2)}</span>\n          </Label>\n          <Slider\n            max={1}\n            min={0}\n            onValueChange={([value]) => onChange({ ...config, topP: value })}\n            step={0.05}\n            value={[config.topP]}\n          />\n        </div>\n      </div>\n\n      <div className=\"flex flex-col gap-2\">\n        <Label className=\"font-semibold\">Stop Sequences</Label>\n        <Textarea\n          onChange={(e) => onChange({ ...config, stopSequences: e.target.value.split(\"\\n\").filter(Boolean) })}\n          placeholder=\"One per line\"\n          rows={3}\n          value={config.stopSequences.join(\"\\n\")}\n          className=\"resize-y\"\n        />\n      </div>\n    </div>\n  )\n}\n",
