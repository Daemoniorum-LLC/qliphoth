    "path": "/home/crook/dev/wraith/wraith-framework/src/domains/devops/components/shared/DataTable.tsx",
    "code": "import React from 'react'\n\ninterface Column<T> {\n  key: string\n  header: string\n  width?: string\n  render?: (item: T) => React.ReactNode\n}\n\ninterface DataTableProps<T> {\n  data: T[]\n  columns: Column<T>[]\n  keyExtractor: (item: T) => string\n  onRowClick?: (item: T) => void\n  emptyMessage?: string\n  compact?: boolean\n}\n\nexport function DataTable<T>({\n  data,\n  columns,\n  keyExtractor,\n  onRowClick,\n  emptyMessage = 'No data available',\n  compact = false,\n}: DataTableProps<T>) {\n  const cellPadding = compact ? 'px-3 py-1.5' : 'px-4 py-2.5'\n\n  if (data.length === 0) {\n    return (\n      <div className=\"flex items-center justify-center h-32 text-[var(--text-muted)] text-sm\">\n        {emptyMessage}\n      </div>\n    )\n  }\n\n  return (\n    <div className=\"overflow-auto\">\n      <table className=\"w-full text-sm\">\n        <thead>\n          <tr className=\"border-b border-[var(--border-primary)]\">\n            {columns.map((col) => (\n              <th\n                key={col.key}\n                className={`${cellPadding} text-left text-xs font-medium text-[var(--text-secondary)] uppercase tracking-wider`}\n                style={{ width: col.width }}\n              >\n                {col.header}\n              </th>\n            ))}\n          </tr>\n        </thead>\n        <tbody className=\"divide-y divide-[var(--border-subtle)]\">\n          {data.map((item) => (\n            <tr\n              key={keyExtractor(item)}\n              onClick={() => onRowClick?.(item)}\n              className={`\n                ${onRowClick ? 'cursor-pointer hover:bg-[var(--bg-hover)]' : ''}\n                transition-colors\n              `}\n            >\n              {columns.map((col) => (\n                <td key={col.key} className={`${cellPadding} text-[var(--text-primary)]`}>\n                  {col.render\n                    ? col.render(item)\n                    : String((item as Record<string, unknown>)[col.key] ?? '')}\n                </td>\n              ))}\n            </tr>\n          ))}\n        </tbody>\n      </table>\n    </div>\n  )\n}\n",
