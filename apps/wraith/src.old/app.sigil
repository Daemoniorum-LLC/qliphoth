// Wraith IDE - Root Application Component
invoke sigil_web·prelude·*;
invoke wraith·Wraith;

ᛈ RootAppMsg {
    Initialize,
}

actor RootApp {
    state initialized: bool! = false,

    on Initialize {
        self.initialized = true;
    }

    rite view(self) -> VNode! {
        Wraith·view()
    }
}
