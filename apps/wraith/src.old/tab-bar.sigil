invoke sigil_web·prelude·*;
rite tab_bar(tabs: Any, active_tab_id: Any, pinned_tabs: Any, on_tab_click: Any, on_tab_close: Any, on_tab_pin: Any, on_tab_context_menu: Any, class_name: Any) -> VNode! {
    VNode·div()
        ·attr("role", "tablist")
        ·attr("aria-label", "Open files")
        ·class("flex items-center bg-[var(--bg-secondary)] border-b border-[var(--border-primary)] overflow-x-auto ")
        ·text_child(None·to_string())
}