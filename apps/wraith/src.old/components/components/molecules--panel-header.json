    "code": "import React from 'react'\nimport { X, ChevronDown, MoreHorizontal } from 'lucide-react'\nimport { IconButton } from '../atoms'\n\nexport interface PanelHeaderProps {\n  title: string\n  icon?: React.ReactNode\n  onClose?: () => void\n  onCollapse?: () => void\n  onMore?: () => void\n  actions?: React.ReactNode\n  className?: string\n}\n\nexport const PanelHeader: React.FC<PanelHeaderProps> = ({\n  title,\n  icon,\n  onClose,\n  onCollapse,\n  onMore,\n  actions,\n  className = '',\n}) => {\n  return (\n    <div\n      className={`flex items-center justify-between px-3 py-2 bg-[var(--bg-secondary)] border-b border-[var(--border-primary)] ${className}`}\n    >\n      <div className=\"flex items-center gap-2 min-w-0\">\n        {icon && (\n          <span className=\"text-[var(--text-secondary)] flex-shrink-0\">\n            {icon}\n          </span>\n        )}\n        <span className=\"text-xs font-semibold text-[var(--text-primary)] truncate\">\n          {title}\n        </span>\n      </div>\n      <div className=\"flex items-center gap-1\">\n        {actions}\n        {onMore && (\n          <IconButton\n            icon={<MoreHorizontal />}\n            size=\"sm\"\n            tooltip=\"More actions\"\n            onClick={onMore}\n          />\n        )}\n        {onCollapse && (\n          <IconButton\n            icon={<ChevronDown />}\n            size=\"sm\"\n            tooltip=\"Collapse panel\"\n            onClick={onCollapse}\n          />\n        )}\n        {onClose && (\n          <IconButton\n            icon={<X />}\n            size=\"sm\"\n            tooltip=\"Close panel\"\n            onClick={onClose}\n          />\n        )}\n      </div>\n    </div>\n  )\n}\n",
