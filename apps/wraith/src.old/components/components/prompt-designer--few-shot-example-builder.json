  "source": {
    "path": "/home/crook/dev/wraith/wraith-framework/src/domains/prompts/components/PromptDesigner/FewShotExampleBuilder.tsx",
    "code": "import { Button, Card, Label, Textarea } from \"@persona-framework/ui\"\nimport { Plus, Trash2 } from \"lucide-react\"\nimport type { FewShotExample } from \"../../types\"\n\ninterface FewShotExampleBuilderProps {\n  examples: FewShotExample[]\n  onChange: (examples: FewShotExample[]) => void\n}\n\nexport function FewShotExampleBuilder({ examples, onChange }: FewShotExampleBuilderProps) {\n  const addExample = () => {\n    onChange([...examples, { id: crypto.randomUUID(), input: \"\", output: \"\" }])\n  }\n\n  const removeExample = (id: string) => {\n    onChange(examples.filter(ex => ex.id !== id))\n  }\n\n  const updateExample = (id: string, field: \"input\" | \"output\", value: string) => {\n    onChange(examples.map(ex => ex.id === id ? { ...ex, [field]: value } : ex))\n  }\n\n  return (\n    <div className=\"flex flex-col gap-3\">\n      <div className=\"flex items-center justify-between gap-2\">\n        <Label className=\"font-semibold\">Few-Shot Examples</Label>\n        <Button onClick={addExample} size=\"sm\">\n          <Plus className=\"mr-2 h-4 w-4\" />\n          Add Example\n        </Button>\n      </div>\n\n      <div className=\"flex flex-col gap-2 max-h-[400px] overflow-auto\">\n        {examples.map((example, index) => (\n          <Card className=\"p-3\" key={example.id}>\n            <div className=\"flex items-center justify-between gap-2 mb-2\">\n              <Label className=\"font-semibold\">\n                Example\n                {\" \"}\n                {index + 1}\n              </Label>\n              <Button\n                onClick={() => removeExample(example.id)}\n                size=\"sm\"\n                variant=\"ghost\"\n              >\n                <Trash2 className=\"h-4 w-4\" />\n              </Button>\n            </div>\n            <div className=\"mb-2\">\n              <Label className=\"text-sm\">Input</Label>\n              <Textarea\n                onChange={(e) => updateExample(example.id, \"input\", e.target.value)}\n                placeholder=\"Example input...\"\n                rows={2}\n                value={example.input}\n                className=\"resize-y\"\n              />\n            </div>\n            <div>\n              <Label className=\"text-sm\">Expected Output</Label>\n              <Textarea\n                onChange={(e) => updateExample(example.id, \"output\", e.target.value)}\n                placeholder=\"Example output...\"\n                rows={2}\n                value={example.output}\n                className=\"resize-y\"\n              />\n            </div>\n          </Card>\n        ))}\n        {examples.length === 0 && (\n          <div className=\"text-muted-foreground p-4 text-center\">\n            No examples defined. Click \"Add Example\" to create one.\n          </div>\n        )}\n      </div>\n    </div>\n  )\n}\n",
