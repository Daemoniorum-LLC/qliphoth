    "code": "import { useState } from 'react'\nimport {\n  GitBranch, Play, Square, RotateCcw, Clock, CheckCircle2, XCircle,\n  Circle, ChevronDown, ChevronRight, ExternalLink, Search, Filter\n} from 'lucide-react'\nimport { IconButton } from '@ui/components/atoms'\nimport { StatusBadge, SectionHeader, ProviderIcon, ActionMenu, ActionItem } from '../shared'\nimport { mockPipelines } from '../../mock-data'\nimport type { Pipeline, PipelineRun, PipelineStage, PipelineStatus } from '../../types'\n\nfunction formatDuration(seconds?: number): string {\n  if (!seconds) return '-'\n  const mins = Math.floor(seconds / 60)\n  const secs = seconds % 60\n  return `${mins}m ${secs}s`\n}\n\nfunction formatRelativeTime(dateStr?: string): string {\n  if (!dateStr) return '-'\n  const diff = Date.now() - new Date(dateStr).getTime()\n  const mins = Math.floor(diff / 60000)\n  const hours = Math.floor(diff / 3600000)\n  if (hours > 0) return `${hours}h ago`\n  if (mins > 0) return `${mins}m ago`\n  return 'Just now'\n}\n\nfunction StageStatusIcon({ status }: { status: PipelineStatus }) {\n  switch (status) {\n    case 'success':\n      return <CheckCircle2 className=\"h-4 w-4 text-[var(--accent-success)]\" />\n    case 'failure':\n      return <XCircle className=\"h-4 w-4 text-[var(--accent-error)]\" />\n    case 'running':\n      return <Circle className=\"h-4 w-4 text-[var(--accent-primary)] animate-pulse\" />\n    case 'pending':\n      return <Circle className=\"h-4 w-4 text-[var(--text-muted)]\" />\n    default:\n      return <Circle className=\"h-4 w-4 text-[var(--text-muted)]\" />\n  }\n}\n\nfunction StagesTimeline({ stages }: { stages: PipelineStage[] }) {\n  return (\n    <div className=\"flex items-center gap-1\">\n      {stages.map((stage, idx) => (\n        <div key={stage.name} className=\"flex items-center\">\n          <div className=\"flex items-center gap-1 px-2 py-1 bg-[var(--bg-tertiary)] rounded\">\n            <StageStatusIcon status={stage.status} />\n            <span className=\"text-xs text-[var(--text-secondary)]\">{stage.name}</span>\n          </div>\n          {idx < stages.length - 1 && (\n            <div className=\"w-4 h-px bg-[var(--border-primary)]\" />\n          )}\n        </div>\n      ))}\n    </div>\n  )\n}\n\nfunction PipelineCard({ pipeline }: { pipeline: Pipeline }) {\n  const [expanded, setExpanded] = useState(false)\n  const run = pipeline.lastRun\n\n  return (\n    <div className=\"bg-[var(--bg-secondary)] rounded-lg border border-[var(--border-primary)] overflow-hidden\">\n      <div\n        className=\"flex items-center justify-between p-3 cursor-pointer hover:bg-[var(--bg-hover)]\"\n        onClick={() => setExpanded(!expanded)}\n      >\n        <div className=\"flex items-center gap-3\">\n          <button className=\"text-[var(--text-secondary)]\">\n            {expanded ? <ChevronDown className=\"h-4 w-4\" /> : <ChevronRight className=\"h-4 w-4\" />}\n          </button>\n          <ProviderIcon provider={pipeline.provider} />\n          <div>\n            <div className=\"flex items-center gap-2\">\n              <span className=\"font-medium text-[var(--text-primary)]\">{pipeline.name}</span>\n              {run && <StatusBadge status={run.status} />}\n            </div>\n            <div className=\"flex items-center gap-2 text-xs text-[var(--text-muted)] mt-0.5\">\n              <GitBranch className=\"h-3 w-3\" />\n              <span>{pipeline.repository}</span>\n              <span className=\"text-[var(--border-primary)]\">â€¢</span>\n              <span>{pipeline.branch}</span>\n            </div>\n          </div>\n        </div>\n        <div className=\"flex items-center gap-4\" onClick={(e) => e.stopPropagation()}>\n          {run && (\n            <div className=\"flex items-center gap-4 text-xs text-[var(--text-secondary)]\">\n              <div className=\"flex items-center gap-1\">\n                <span className=\"font-mono\">#{run.number}</span>\n              </div>\n              <div className=\"flex items-center gap-1\">\n                <Clock className=\"h-3 w-3\" />\n                <span>{formatDuration(run.duration)}</span>\n              </div>\n              <span>{formatRelativeTime(run.startedAt)}</span>\n            </div>\n          )}\n          <div className=\"flex items-center gap-1\">\n            <IconButton icon={<Play className=\"h-3 w-3\" />} size=\"sm\" tooltip=\"Run pipeline\" onClick={() => {}} />\n            <IconButton icon={<ExternalLink className=\"h-3 w-3\" />} size=\"sm\" tooltip=\"Open in browser\" onClick={() => {}} />\n            <ActionMenu>\n              <ActionItem icon={<Play className=\"h-3 w-3\" />} label=\"Run\" onClick={() => {}} />\n              <ActionItem icon={<Square className=\"h-3 w-3\" />} label=\"Cancel\" onClick={() => {}} />\n              <ActionItem icon={<RotateCcw className=\"h-3 w-3\" />} label=\"Retry\" onClick={() => {}} />\n            </ActionMenu>\n          </div>\n        </div>\n      </div>\n\n      {expanded && run && (\n        <div className=\"border-t border-[var(--border-subtle)] bg-[var(--bg-primary)] p-4\">\n          <div className=\"mb-3\">\n            <div className=\"flex items-center gap-2 mb-2\">\n              <span className=\"text-xs font-medium text-[var(--text-secondary)]\">Latest Run</span>\n              <span className=\"text-xs font-mono text-[var(--text-muted)]\">{run.commit}</span>\n            </div>\n            <p className=\"text-sm text-[var(--text-primary)]\">{run.commitMessage}</p>\n            <p className=\"text-xs text-[var(--text-muted)] mt-1\">by {run.author}</p>\n          </div>\n\n          <div className=\"mb-3\">\n            <span className=\"text-xs font-medium text-[var(--text-secondary)] block mb-2\">Stages</span>\n            <StagesTimeline stages={run.stages} />\n          </div>\n\n          <div className=\"flex items-center gap-4 text-xs text-[var(--text-muted)]\">\n            <div>Started: {formatRelativeTime(run.startedAt)}</div>\n            {run.finishedAt && <div>Finished: {formatRelativeTime(run.finishedAt)}</div>}\n            <div>Duration: {formatDuration(run.duration)}</div>\n          </div>\n        </div>\n      )}\n    </div>\n  )\n}\n\nexport function PipelinesSection() {\n  const [filter, setFilter] = useState('')\n  const [statusFilter, setStatusFilter] = useState<PipelineStatus | 'all'>('all')\n\n  const filteredPipelines = mockPipelines.filter((p) => {\n    const matchesSearch = filter\n      ? p.name.toLowerCase().includes(filter.toLowerCase()) ||\n        p.repository.toLowerCase().includes(filter.toLowerCase())\n      : true\n    const matchesStatus = statusFilter === 'all' || p.lastRun?.status === statusFilter\n    return matchesSearch && matchesStatus\n  })\n\n  return (\n    <div className=\"h-full flex flex-col\">\n      <SectionHeader\n        title=\"CI/CD Pipelines\"\n        subtitle={`${filteredPipelines.length} pipeline${filteredPipelines.length !== 1 ? 's' : ''}`}\n        icon={<GitBranch className=\"h-5 w-5\" />}\n        onRefresh={() => {}}\n        actions={\n          <div className=\"flex items-center gap-2\">\n            <div className=\"relative\">\n              <Search className=\"h-3.5 w-3.5 absolute left-2 top-1/2 -translate-y-1/2 text-[var(--text-muted)]\" />\n              <input\n                type=\"text\"\n                placeholder=\"Filter...\"\n                value={filter}\n                onChange={(e) => setFilter(e.target.value)}\n                className=\"pl-7 pr-3 py-1 text-xs bg-[var(--bg-primary)] border border-[var(--border-primary)] rounded w-40 text-[var(--text-primary)] placeholder:text-[var(--text-muted)] focus:outline-none focus:border-[var(--accent-primary)]\"\n              />\n            </div>\n            <select\n              value={statusFilter}\n              onChange={(e) => setStatusFilter(e.target.value as PipelineStatus | 'all')}\n              className=\"px-2 py-1 text-xs bg-[var(--bg-primary)] border border-[var(--border-primary)] rounded text-[var(--text-primary)] focus:outline-none focus:border-[var(--accent-primary)]\"\n            >\n              <option value=\"all\">All Status</option>\n              <option value=\"success\">Success</option>\n              <option value=\"failure\">Failed</option>\n              <option value=\"running\">Running</option>\n              <option value=\"pending\">Pending</option>\n            </select>\n          </div>\n        }\n      />\n\n      <div className=\"flex-1 overflow-auto space-y-3 pr-1\">\n        {filteredPipelines.map((pipeline) => (\n          <PipelineCard key={pipeline.id} pipeline={pipeline} />\n        ))}\n      </div>\n    </div>\n  )\n}\n",
