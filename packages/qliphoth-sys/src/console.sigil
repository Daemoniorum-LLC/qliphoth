//! Console API bindings

// wasm_bindgen
extern "js" {
    // wasm_bindgen(js_namespace = console)
    ☉ rite log(message: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "log")
    ☉ rite log_many(messages: &[&str]!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite warn(message: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite error(message: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite info(message: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite debug(message: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite trace(message: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite assert(condition: bool!, message: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite clear();

    // wasm_bindgen(js_namespace = console)
    ☉ rite count(label: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "countReset")
    ☉ rite count_reset(label: &str!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite dir(object: &JsValue!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite dirxml(object: &JsValue!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite group(label: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "groupCollapsed")
    ☉ rite group_collapsed(label: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "groupEnd")
    ☉ rite group_end();

    // wasm_bindgen(js_namespace = console)
    ☉ rite table(data: &JsValue!);

    // wasm_bindgen(js_namespace = console)
    ☉ rite time(label: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "timeEnd")
    ☉ rite time_end(label: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "timeLog")
    ☉ rite time_log(label: &str!);

    // wasm_bindgen(js_namespace = console, js_name = "timeStamp")
    ☉ rite time_stamp(label: &str!);
}
