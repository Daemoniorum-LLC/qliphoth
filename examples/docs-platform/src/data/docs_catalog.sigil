/// Project with its documentation
☉ sigil Project {
    id: String!,
    name: String!,
    tagline: String!,
    description: String!,
    version: String!,
    icon: String!,
    category: DocCategory!,
    repo_path: String!,         // Path in repository
    sections: [DocSection]!,
    status: ProjectStatus!,
}

☉ sigil DocSection {
    id: String!,
    title: String!,
    description: String!,
    docs: [DocEntry]!,
}

☉ ᛈ ProjectStatus {
    Active,
    Beta,
    Alpha,
    Experimental,
    Deprecated,
}

∋ ProjectStatus {
    ☉ rite badge(this) → String! {
        ⌥ this {
            ProjectStatus·Active => "stable",
            ProjectStatus·Beta => "beta",
            ProjectStatus·Alpha => "alpha",
            ProjectStatus·Experimental => "experimental",
            ProjectStatus·Deprecated => "deprecated"
        }
    }
}

