/// Get all projects grouped by category
☉ rite get_projects_by_category() → HashMap<DocCategory, [Project]>! {
    ≔ vary grouped! = HashMap·new();
    ∀ project ∈ DOCS_CATALOG·iter() {
        grouped·entry(project.category·clone())·or_default()·push(project·clone());
    }
    grouped
}

/// Search documentation
☉ rite search_docs(query: String) → [DocEntry]! {
    ≔ query_lower! = query·to_lowercase();

    DOCS_CATALOG·iter()
        ·flat_map(|p| p.sections·iter()·flat_map(|s| s.docs·iter()))
        ·filter(|doc| {
            doc.title·to_lowercase()·contains(query_lower·as_str()) ∨
            doc.description·to_lowercase()·contains(query_lower·as_str()) ∨
            doc.tags·iter()·any(|t| t·to_lowercase()·contains(query_lower·as_str()))
        })
        ·collect()
}

/// Get all documentation entries
☉ rite get_all_docs() → [DocEntry]! {
    DOCS_CATALOG·iter()
        ·flat_map(|p| p.sections·iter()·flat_map(|s| s.docs·iter()))
        ·collect()
}

// ============================================================================
// Option Types
// ============================================================================

ᛈ OptionString {
    None,
    Some(String),
}

rite option_string_is_some(opt: OptionString) → bool! {
    ⌥ opt {
        OptionString·Some(_) => yea,
        OptionString·None => nay
    }
}

rite option_string_unwrap(opt: OptionString) → String! {
    ⌥ opt {
        OptionString·Some(v) => v,
        OptionString·None => ""
    }
}

