/// Documentation entry representing a single document
☉ sigil DocEntry {
    id: String!,
    title: String!,
    path: String!,              // URL path
    file_path: String!,         // Actual file path in repo
    description: String!,
    category: DocCategory!,
    tags: [String]!,
    last_updated: OptionString?,
}

