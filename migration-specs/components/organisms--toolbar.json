  "source": {
    "path": "/home/crook/dev/wraith/wraith-framework/src/ui/components/organisms/Toolbar.tsx",
    "code": "import React from 'react'\nimport { Plus, ChevronUp, ChevronDown, Command } from 'lucide-react'\nimport { Button } from '@persona-framework/ui/button'\nimport { DropdownMenu, DropdownMenuTrigger, DropdownMenuContent, DropdownMenuItem } from '@persona-framework/ui/dropdown-menu'\nimport { KeyboardShortcut } from '../atoms'\n\nexport interface ToolbarPanel {\n  id: string\n  name: string\n}\n\nexport interface ToolbarProps {\n  collapsed: boolean\n  onToggleCollapse: () => void\n  onCommandPalette: () => void\n  availablePanels: ToolbarPanel[]\n  onAddToLeft: (panelId: string) => void\n  onAddToRight: (panelId: string) => void\n  panelCount: number\n  tabCount: number\n  className?: string\n}\n\nexport const Toolbar: React.FC<ToolbarProps> = ({\n  collapsed,\n  onToggleCollapse,\n  onCommandPalette,\n  availablePanels,\n  onAddToLeft,\n  onAddToRight,\n  panelCount,\n  tabCount,\n  className = '',\n}) => {\n  if (collapsed) {\n    return (\n      <div className={`py-0.5 px-2 bg-[var(--bg-secondary)] border-b border-[var(--border-primary)] flex justify-center ${className}`}>\n        <Button\n          variant=\"ghost\"\n          size=\"sm\"\n          onClick={onToggleCollapse}\n          title=\"Expand toolbar\"\n        >\n          <ChevronDown className=\"h-4 w-4\" />\n        </Button>\n      </div>\n    )\n  }\n\n  return (\n    <div className={`flex gap-2 px-3 py-2 bg-[var(--bg-secondary)] border-b border-[var(--border-primary)] items-center text-xs text-[var(--text-primary)] ${className}`}>\n      <span className=\"font-semibold text-[var(--accent-primary)]\">Wraith</span>\n      <span className=\"text-[var(--text-muted)]\">|</span>\n\n      <button\n        onClick={onCommandPalette}\n        className=\"flex items-center gap-1.5 text-[var(--text-secondary)] hover:text-[var(--text-primary)] transition-colors\"\n        title=\"Open Command Palette\"\n      >\n        <Command className=\"h-3.5 w-3.5\" />\n        <KeyboardShortcut keys={['Ctrl', 'Shift', 'P']} />\n      </button>\n\n      <div className=\"flex-1\" />\n\n      {availablePanels.length > 0 && (\n        <>\n          <DropdownMenu>\n            <DropdownMenuTrigger asChild>\n              <Button variant=\"ghost\" size=\"sm\" className=\"gap-1\" title=\"Add panel to left sidebar\">\n                <Plus className=\"h-4 w-4\" />\n                Add to Left\n              </Button>\n            </DropdownMenuTrigger>\n            <DropdownMenuContent>\n              {availablePanels.map((panel) => (\n                <DropdownMenuItem key={panel.id} onClick={() => onAddToLeft(panel.id)}>\n                  {panel.name}\n                </DropdownMenuItem>\n              ))}\n            </DropdownMenuContent>\n          </DropdownMenu>\n\n          <DropdownMenu>\n            <DropdownMenuTrigger asChild>\n              <Button variant=\"ghost\" size=\"sm\" className=\"gap-1\" title=\"Add panel to right sidebar\">\n                <Plus className=\"h-4 w-4\" />\n                Add to Right\n              </Button>\n            </DropdownMenuTrigger>\n            <DropdownMenuContent>\n              {availablePanels.map((panel) => (\n                <DropdownMenuItem key={panel.id} onClick={() => onAddToRight(panel.id)}>\n                  {panel.name}\n                </DropdownMenuItem>\n              ))}\n            </DropdownMenuContent>\n          </DropdownMenu>\n        </>\n      )}\n\n      <span className=\"text-[11px] text-[var(--text-secondary)]\">\n        {panelCount} panels | {tabCount} tabs\n      </span>\n\n      <Button\n        variant=\"ghost\"\n        size=\"sm\"\n        onClick={onToggleCollapse}\n        className=\"ml-2\"\n        title=\"Collapse toolbar\"\n      >\n        <ChevronUp className=\"h-4 w-4\" />\n      </Button>\n    </div>\n  )\n}\n",
