    "code": "import React from 'react'\nimport { PanelHeader } from '../molecules'\nimport { ResizeHandle } from '../molecules'\nimport { Skeleton } from '../atoms'\n\nexport interface PanelContainerProps {\n  title: string\n  icon?: React.ReactNode\n  children: React.ReactNode\n  side: 'left' | 'right'\n  width: number\n  minWidth?: number\n  maxWidth?: number\n  isNarrow?: boolean\n  loading?: boolean\n  onClose: () => void\n  onCollapse: () => void\n  onResize: (delta: number) => void\n  className?: string\n}\n\nexport const PanelContainer: React.FC<PanelContainerProps> = ({\n  title,\n  icon,\n  children,\n  side,\n  width,\n  minWidth = 200,\n  maxWidth = 500,\n  isNarrow = false,\n  loading = false,\n  onClose,\n  onCollapse,\n  onResize,\n  className = '',\n}) => {\n  const handleResize = (delta: number) => {\n    // For right panel, invert the delta\n    const adjustedDelta = side === 'right' ? -delta : delta\n    onResize(adjustedDelta)\n  }\n\n  return (\n    <div\n      className={`flex flex-col overflow-hidden transition-[width,opacity] duration-300 ${\n        isNarrow\n          ? 'absolute top-12 left-0 right-0 h-[40vh] w-auto max-w-none z-[100] border-b-2 shadow-[var(--shadow-panel)]'\n          : 'relative'\n      } ${side === 'right' ? 'border-l border-[var(--border-primary)]' : ''} ${className}`}\n      style={isNarrow ? {} : { width: `${width}px`, minWidth: `${minWidth}px`, maxWidth: `${maxWidth}px` }}\n    >\n      {/* Resize handle */}\n      {!isNarrow && (\n        <ResizeHandle\n          direction=\"horizontal\"\n          onResize={handleResize}\n          className={`absolute top-0 bottom-0 ${side === 'left' ? 'right-0' : 'left-0'}`}\n        />\n      )}\n\n      <div className=\"flex flex-col flex-1 overflow-hidden\">\n        <PanelHeader\n          title={title}\n          icon={icon}\n          onClose={onClose}\n          onCollapse={onCollapse}\n        />\n\n        <div className=\"flex-1 overflow-auto bg-[var(--bg-primary)]\">\n          {loading ? (\n            <div className=\"p-4\">\n              <Skeleton variant=\"text\" className=\"mb-2\" />\n              <Skeleton variant=\"text\" className=\"mb-2\" width=\"80%\" />\n              <Skeleton variant=\"text\" width=\"60%\" />\n            </div>\n          ) : (\n            children\n          )}\n        </div>\n      </div>\n    </div>\n  )\n}\n",
