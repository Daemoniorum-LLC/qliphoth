    "code": "import React from 'react'\nimport { ExtensionCard } from '../molecules'\n\nexport interface Extension {\n  id: string\n  name: string\n  description: string\n  icon: React.ReactNode\n  category: string\n}\n\nexport interface ExtensionsPanelProps {\n  extensions: Extension[]\n  enabledExtensions: string[]\n  onToggleExtension: (extensionId: string) => void\n  className?: string\n}\n\nexport const ExtensionsPanel: React.FC<ExtensionsPanelProps> = ({\n  extensions,\n  enabledExtensions,\n  onToggleExtension,\n  className = '',\n}) => {\n  // Group extensions by category\n  const categories = [...new Set(extensions.map((e) => e.category))]\n\n  const enabledCount = enabledExtensions.length\n  const totalCount = extensions.length\n\n  return (\n    <div className={`p-3 space-y-4 ${className}`}>\n      <div className=\"flex items-center justify-between\">\n        <p className=\"text-xs text-[var(--text-secondary)]\">\n          Enable extensions to add functionality\n        </p>\n        <span className=\"text-xs text-[var(--text-muted)]\">\n          {enabledCount}/{totalCount} enabled\n        </span>\n      </div>\n\n      {categories.map((category) => {\n        const categoryExtensions = extensions.filter((e) => e.category === category)\n        if (categoryExtensions.length === 0) return null\n\n        return (\n          <div key={category}>\n            <h3 className=\"text-xs font-semibold text-[var(--text-secondary)] uppercase tracking-wider mb-2\">\n              {category}\n            </h3>\n            <div className=\"space-y-1\">\n              {categoryExtensions.map((ext) => (\n                <ExtensionCard\n                  key={ext.id}\n                  id={ext.id}\n                  name={ext.name}\n                  description={ext.description}\n                  icon={ext.icon}\n                  enabled={enabledExtensions.includes(ext.id)}\n                  onToggle={() => onToggleExtension(ext.id)}\n                />\n              ))}\n            </div>\n          </div>\n        )\n      })}\n    </div>\n  )\n}\n",
